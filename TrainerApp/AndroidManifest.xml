<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trainerapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_laucher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.trainerapp.ActivityMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.trainerapp.ActivityCrearEntrenamiento" android:screenOrientation="portrait"></activity>
        <activity android:name="com.trainerapp.ActivityVerRutina" android:screenOrientation="portrait" ></activity>
        <activity android:name="com.trainerapp.ActivityDiaEntrenamiento" android:screenOrientation="portrait" ></activity>
        <activity android:name="com.trainerapp.ActivityZonasMusculares" android:screenOrientation="portrait" ></activity>
        <activity android:name="com.trainerapp.ActivityListaEjercicios" android:screenOrientation="portrait" ></activity>
        <activity android:name="com.trainerapp.ActivityDescripcion" android:screenOrientation="portrait" ></activity>
         <activity android:name="com.trainerapp.ScreenSlideActivity" android:screenOrientation="portrait" ></activity>
    </application>

</manifest>
